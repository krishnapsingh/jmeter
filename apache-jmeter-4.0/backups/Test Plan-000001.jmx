	failureMessage += &quot;Expected &lt;response code&gt; [200] but we got instead [&quot; + prev.getResponseCode() + &quot;]\n\n&quot; ;
}

if (jsonResponse.status.message != &quot;the daily limit of 30000 credits for demo has been exceeded. Please use an application specific account. Do not use the demo account for your application.&quot;) {
	failureMessage += &quot;The element has wrong number of servers. Number of servers is:[&quot; + jsonResponse.status.message + &quot;]\n\n&quot;;
} 

if (failureMessage?.trim()) {
	AssertionResult.setFailureMessage(failureMessage);
